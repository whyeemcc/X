.param devtp
.param vdd
.param scale_normalize
.param normalize_size
.param forvd

.param vgsweep=0

.dc vgsweep 0 vdd vdd

xM~ D~ G~ S~ B~ model_name dev_param
VDS~ D~ 0 'forvd*<vdd>*devtp'
VGS~ G~ 0 0
VS~ S~ 0 0
VBS~ B~ 0 0

.meas dc igoff~ param='devtp*i(VGS~)/<NFIN>/scale_normalize*1e12'           $unit(pA/fin)
