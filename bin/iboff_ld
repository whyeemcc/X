.param devtp
.param vdd_drain
.param vdd_gate
.param scale_normalize
.param normalize_size
.param forvd

.param vgsweep=0

.dc vgsweep 0 vdd_drain vdd_drain
 
xM~ D~ G~ S~ B~ model_name dev_param
VDS~ D~ 0 'forvd*<vdd_drain>*devtp'
VGS~ G~ 0 0
VS~ S~ 0 0
VBS~ B~ 0 0

.meas dc iboff_ld~ param='devtp*i(VBS~)/<NFIN>/scale_normalize*1e12'                 $unit(pA/fin)
