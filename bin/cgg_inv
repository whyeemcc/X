.param devtp
.param vdd
.param scale_normalize
.param normalize_size

.param NF=1
.param MR=1

.AC POI 1 100000

xM~ D~ G~ S~ B~ model_name dev_param
VD~ D~ S~ 0
VB~ B~ S~ 0
VS~ S~ 0 dc=0 ac=0.1
VG~ G~ 0 '<vdd>*devtp'

.meas ac IC~ find II(VG~) AT=100000
.meas ac cgg_inv~ param='IC~/0.1/(2*3.14159265358*100000)*1e15/<NFIN>/<NF>/<MR>/scale_normalize/scale_normalize'      $unit(fF/fin)
