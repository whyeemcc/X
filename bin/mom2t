.param Vmom
.param scale_normalize
.param normalize_size

.AC POI 1 100000

xM~ D~ 0 model_name dev_param

VDG1~ D~ DT~ dc=0 ac=0.1
VDG2~ DT~ 0 dc='Vmom'

.meas ac i_in~ find II(VDG1~) AT=100000
.meas ac mom2t~ param='-i_in~/0.1/(2*3.14159265358*100000)*1e15'                   $unit(fF)
