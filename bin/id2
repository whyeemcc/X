.param devtp
.param vdd
.param scale_normalize
.param normalize_size

.param vgsweep=0

.dc vgsweep 0 vdd vdd

xM~ D~ G~ 0 0 model_name dev_param
VDS~ D~ 0 '<vdd>*devtp'
VGS~ G~ 0 '0.5*<vdd>*devtp'

.meas dc id2~ param='-devtp*i(VDS~)/<NFIN>/scale_normalize*1e6'              $unit(uA/fin)
